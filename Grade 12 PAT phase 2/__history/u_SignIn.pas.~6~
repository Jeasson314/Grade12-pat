unit u_SignIn;

interface

type
  SignIn = class(tObject)
  private
    fFirst_name: string;
    flast_name: string;
    fUsername: String;
    fPassword: string;
    fAttempts:string;
  public
  procedure SetLogin(sUsername,sPassword:string);
  Function PasswordCheck: boolean; //checks if entered password is correc
  end;

implementation

{ SignIn }

procedure SignIn.SetLogin(sUsername, sPassword: string);
begin
fUsername:=sUsername;
fPassword:=sPassword;
end;



end.
