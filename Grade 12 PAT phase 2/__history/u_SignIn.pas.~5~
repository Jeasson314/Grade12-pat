unit u_SignIn;

interface

type
  SignIn = class(tObject)
  private
    fFirst_name: string;
    flast_name: string;
    Fusername: String;
    Fpassword: string;
  public
  procedure SetLogin(sUsername,sPassword:string);
  end;

implementation

{ SignIn }

procedure SignIn.SetLogin(sUsername, sPassword: string);
begin
fUsername:=sUsername;
fPassword:=sPassword;
end;



end.
